{
    "name": "syyong/phspring",
    "description": "The phspring framework.",
    "keywords": [
        "phspring",
        "PHP socket framework"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "lib-pcre": "*"
    },
    "require-dev": {
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "phspring\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
